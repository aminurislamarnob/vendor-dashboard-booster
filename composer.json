{
    "name": "welabs/dokan-customers",
    "description": "A dokan addon plugin which enable vendor wise customers features on each vendor panel.",
    "license": "GPLv2 or later",
    "autoload": {
        "psr-4": {
            "WeLabs\\DokanCustomers\\": "includes/"
        }
    },
    "authors": [
        {
            "name": "Aminur Islam Arnob",
            "email": "eng.aminurislam@gmail.com",
            "homepage": "https://wordpress.org/plugins/dokan-customers"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "wp-coding-standards/wpcs": "dev-develop",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "tareq1988/wp-php-cs-fixer": "dev-master",
        "phpcompatibility/phpcompatibility-wp": "dev-master",
        "phpunit/phpunit": "~8.5",
        "wp-phpunit/wp-phpunit": "^6.1",
        "yoast/phpunit-polyfills": "^1.0"
    },
   "scripts": {
        "phpcs": [
            "vendor/bin/phpcs -p -s"
        ],
        "phpcs:report": [
            "vendor/bin/phpcs --report-file='phpcs-report.txt'"
        ],
        "phpcbf": [
            "vendor/bin/phpcbf -p"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
